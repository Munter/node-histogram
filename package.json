    "jpeg",
    "jpg",
    "gif"
  ],
  "maintainers": [
    {
      "name": "Peter Müller",
      "email": "munter@fumle.dk",
      "web": "http://fumle.dk"
    }
  ],
  "licenses": [
    {
      "type": "BeerWare"
    }
  ],
  "homepage": "http://munter.github.com/node-histogram",
  "repository": {
    "type": "git",
    "url": "git://github.com/Munter/node-histogram.git"
  },
  "files": [
    "lib",
    "histogram.min.js"
  ],
  "main": "lib/index.js",
  "dependencies": {
    "canvas": "^1.1.6"
  },
  "devDependencies": {
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0",
    "jshint": "^2.5.3",
    "mocha": "^1.21.4",
    "mocha-lcov-reporter": "0.0.1",
    "mocha-phantomjs": "^3.5.2",
    "phantomjs": "^1.9.13",
    "uglify-js": "^2.4.15",
    "unexpected": "^10.0.1"
  },
  "optionalDependencies": {},
  "engines": {
    "node": "*"
  },
  "scripts": {
    "lint": "jshint .",
